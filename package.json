{
  "name": "project_rumor",
  "productName": "Project_Rumor",
  "version": "0.0.1",
  "description": "Rumor has it that some guy started a project named ProjectRumor",
  "main": "es6-init.js",
  "scripts": {
    "start": "electron .",
    "test": "jest",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --no-prune --out=build",
    "package-win": "electron-packager . project_rumor --overwrite --asar --platform=win32 --arch=ia32 --no-prune --out=build --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Project Rumor\"",
    "package-linux": "electron-packager . project_rumor --overwrite --asar --platform=linux --arch=x64 --no-prune --out=build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainybreeze777/ProjectRumor"
  },
  "keywords": [
    "Rumor"
  ],
  "author": "Siting Pan",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "electron-compilers": "^5.9.0",
    "electron-packager": "^12.1.2",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.4.1",
    "jquery": "^3.3.1"
  },
  "dependencies": {
    "electron": "^3.0.2",
    "electron-compile": "^6.4.3",
    "electron-debug": "^2.0.0",
    "jsep": "^0.3.4"
  },
  "directories": {
    "test": "test"
  }
}
